{
  "manifest_version": 2,
 
  "name": "Block Social",
  "description": "NS Social Distraction Blocker",
  "version": "2.1",
 
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html",
   "icons": {"16": "icon16.png"}
  },
 "background": {
      "scripts": ["jquery.min.js","background.js"]
    },

  "permissions": [
    "webRequestBlocking",
     "webRequest",
     "activeTab",
     "tabs",
     "http://*/*",
     "https://*/*"
  ],
  "icons": {
      "16": "icon16.png"
    }
}